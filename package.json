{
  "name": "marked-react",
  "description": "Render Markdown as React components",
  "version": "0.1.0",
  "type": "module",
  "author": "sibiraj-s",
  "license": "MIT",
  "repository": "https://github.com/sibiraj-s/marked-react.git",
  "bugs": "https://github.com/sibiraj-s/marked-react/issues",
  "homepage": "https://github.com/sibiraj-s/marked-react#readme",
  "main": "./dist/marked-react.cjs",
  "module": "./dist/marked-react.js",
  "exports": {
    ".": "./dist/marked-react.js",
    "./package.json": "./package.json"
  },
  "typings": "./src/index.d.ts",
  "files": [
    "src/",
    "dist/"
  ],
  "keywords": [
    "react",
    "markdown",
    "marked",
    "markedjs",
    "no dangerouslySetInnerHTML"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "serve": "vite --port 5476",
    "build:docs": "vite build --outDir docs",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "is-ci || npm run build"
  },
  "peerDependencies": {
    "marked": "^3.0.0",
    "react": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/jest": "^27.0.2",
    "@vitejs/plugin-react": "^1.0.6",
    "eslint": "^7.32.0",
    "eslint-config-pegasus": "^2.1.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.3.1",
    "lint-staged": "11.2.6",
    "marked": "^3.0.8",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.58.3",
    "vite": "^2.6.12"
  }
}
