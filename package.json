{
  "name": "marked-react",
  "description": "Render Markdown as React components",
  "version": "1.0.3",
  "type": "module",
  "author": "sibiraj-s",
  "license": "MIT",
  "repository": "https://github.com/sibiraj-s/marked-react.git",
  "bugs": "https://github.com/sibiraj-s/marked-react/issues",
  "homepage": "https://github.com/sibiraj-s/marked-react#readme",
  "main": "./dist/marked-react.cjs",
  "module": "./dist/marked-react.js",
  "exports": {
    ".": "./dist/marked-react.js",
    "./package.json": "./package.json"
  },
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "files": [
    "src/index.d.ts",
    "dist/"
  ],
  "keywords": [
    "react",
    "markdown",
    "marked",
    "markedjs",
    "no dangerouslySetInnerHTML"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "build": "rollup -c",
    "storybook": "start-storybook -p 7007",
    "build:storybook": "build-storybook",
    "test": "jest",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "is-ci || npm run build"
  },
  "peerDependencies": {
    "react": "^16.8.0 || >=17.0.0"
  },
  "dependencies": {
    "marked": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@jest/globals": "^27.5.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-essentials": "^6.5.0-alpha.54",
    "@storybook/react": "^6.5.0-alpha.54",
    "babel-loader": "^8.2.4",
    "commitlint-config-non-conventional": "^1.0.1",
    "eslint": "^8.12.0",
    "eslint-config-pegasus": "^3.3.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-storybook": "^0.5.7",
    "highlight.js": "^11.5.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lint-staged": "12.3.7",
    "prettier": "^2.6.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-lowlight": "^3.0.0",
    "rollup": "^2.70.1"
  }
}
